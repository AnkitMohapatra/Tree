#include"stdafx.h"
#include<iostream>
using namespace std;

struct node
{
	int data;
	node *left;
	node *right;
};

node *GetNewNode(int data)
{
	node *NewNode = new node();
	NewNode->data = data;
	NewNode->left = NewNode->right = NULL;
	return NewNode;
}

node* insert(node *root, int data)
{
	if (root == NULL)
	{
		root = GetNewNode(data);
		return root;
	}
	else
		if (data <= root->data)
		{
			root->left = insert(root->left, data);
		}
		else
			root->right = insert(root->right, data);
	return root;
}

int main()
{
	node *root = NULL; //creating an empty tree
	root = insert(root, 15);
	root = insert(root, 10);
	root = insert(root, 20);
		
}
